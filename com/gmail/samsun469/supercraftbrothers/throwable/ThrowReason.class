package com.gmail.samsun469.supercraftbrothers.throwable;

public enum ThrowReason
{
  DWELLER(new DwellerBone()),  SPAWNEGG(new Spawnegg()),  TNT(new TNT()),  SETHDIODE(new SethDiode()),  SETHCOMPARATOR(new SethComparator()),  SETHREDSTONE(new SethRedstone());
  
  private EntityHitEvent e;
  
  private ThrowReason(EntityHitEvent e)
  {
    this.e = e;
  }
  
  public EntityHitEvent getEvent()
  {
    return this.e;
  }
}
